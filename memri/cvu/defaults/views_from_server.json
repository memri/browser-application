                        "EditorRow", {
                            "title": "{.type}"
                        }, [
                            "VStack", {
                                "cornerRadius": 5
                            }, [
                                "Text", {
                                    "text": "{.handle}",
                                }
                            ]
                        ]
                    ],
                    "websites2":[
                        "EditorRow", {
                            "title": "{.type}"
                        }, [
                            "VStack", {
                                "cornerRadius": 5
                            }, [
                                "Text", {
                                    "text": "{.url}",
                                }
                            ]
                        ]
                    ],
                    "medicalConditions2":[
                        "EditorRow", {}, [
                            "VStack", {
                                "cornerRadius": 5
                            }, [
                                "Text", {
                                    "text": "{.name}",
                                }
                            ]
                        ]
                    ],
                    "companies2":[
                        "EditorRow", {
                            "title": "{.type}"
                        }, [
                            "VStack", {
                                "cornerRadius": 5
                            }, [
                                "Text", {
                                    "text": "{.name}",
                                }
                            ]
                        ]
                    ],
                    "diets2":[
