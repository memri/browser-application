                                        "color": "#434343",
                                        "align": "center",
                                        "font": [14, "regular"]
                                    }, [
                                        "Text", { "text": "Edit" }
                                    ]
                                ]
                            ]
                        ]
                    ],
                    "labels": [
                        "VSTack", {
                            "for": "group",
                            "sectionTitle": "",
                            "padding": [10, 36, 5, 36]
                        }, [
                            "Text", {
                                "condition": "{!.labels.count}",
                                "text": "no labels yet"
                            },
                            "FlowStack", {
                                "list": "{.labels}"
                            }, [
                                "button", {
                                    "press": {
                                        "actionName": "openViewByName",
                                        "actionArgs": [
                                            "all-items-with-label",
                                            {
                                                "name": "Soccer team",
                                                "uid": "0x0124"
                                            }
                                        ]
                                    }
                                }, [
                                    "VStack", {
                                        "background": "{.color}",
                                        "cornerRadius": 5
                                    }, [
                                        "Text", {
                                            "text": "{.name}",
                                            "font": [16, "semibold"],
                                            "color": "#fff",
                                            "padding": [5, 8, 5, 8]
                                        }
                                    ]
                                ]
                            ]
                        ]
                    ],
                    "phoneNumbers2":[
                        "EditorRow", {
                            "title": "{.type}"
                        }, [
                            "Text", {
                                "text": "{.number}",
                            }
                        ]
                    ],
                    "addresses2":[
                        "EditorRow", {
                            "title": "{.type}"
                        }, [
                            "VStack", {
                                "cornerRadius": 5
                            }, [
                                "Text", {
                                    "text": "{.street}",
                                }
                            ]
                        ]
                    ],
                    "publicKeys":[
