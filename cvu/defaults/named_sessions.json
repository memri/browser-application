[
    {
        "name": "shopping-notes",
        "views": [
            "all-notes",
            {
                "title": "Shopping Notes",
                "datasource": {
                    "query": "note AND title BEGINSWITH[c] 'shopping'"
                }
            }
        ]
    },
    
    {
        "name": "all-people",
        "views": [
            {
                "title": "All People",
                "datasource": {
                    "query": "person"
                }
            }
        ]
    },
    
    {
        "name": "all-photos",
        "views": [
            {
                "title": "All Photos",
                "datasource": {
                    "query": "photo"
                }
            }
        ]
    },
    
    {
        "name": "all-addresses",
        "views": [
            {
                "title": "All Addresses",
                "datasource": {
                    "query": "address"
                }
            }
        ]
    },
    
    {
        "name": "all-items-with-label",
        "title": "Items with label {name}",
        "datasource": {
            "query": "* AND ANY labels.uid = {uid}"
        },
        "rendererName": "list",
        "renderConfigs": {
            "list": [
                "ItemCell", {
                    "rendererNames": ["list", "thumbnail"],
                    "variables": "{variables}"
                }
            ]
        }
    }
]
